{
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "plugins": ["react"],
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "rules": {
    "no-undef": "error",
    "no-unused-vars": "warn",
    "no-unreachable": "warn",
    "no-console": "off",
    "no-use-before-define": "error",
    "react/jsx-uses-vars": "error",
    "react/jsx-no-undef": "error"
  }
}
